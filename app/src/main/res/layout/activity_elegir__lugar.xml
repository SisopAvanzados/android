<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:screenOrientation="portrait"
    tools:context="com.example.brian.Estacionamiento.elegir_Lugar">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="79dp"
        android:layout_height="77dp"
        app:srcCompat="@drawable/flechas"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintRight_toLeftOf="@+id/Lugar3"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="79dp"
        android:layout_height="77dp"
        app:srcCompat="@drawable/flechas"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/Lugar2"
        android:layout_marginRight="8dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="79dp"
        android:layout_height="77dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/Lugar1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/flecha_final" />

    <Button
        android:id="@+id/button4"
        android:layout_width="200dp"
        android:layout_height="77dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/background_dark"
        android:elevation="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Lugar4" />

    <Button
        android:id="@+id/Lugar4"
        android:layout_width="200dp"
        android:layout_height="77dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/verde"
        android:onClick="interactuar"
        android:text="LUGAR 4"
        android:textSize="18sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Lugar3" />

    <Button
        android:id="@+id/Lugar3"
        android:layout_width="200dp"
        android:layout_height="77dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/verde"
        android:onClick="interactuar"
        android:text="LUGAR 3"
        android:textSize="18sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Lugar2" />

    <Button
        android:id="@+id/Lugar2"
        android:layout_width="200dp"
        android:layout_height="77dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/verde"
        android:onClick="interactuar"
        android:text="LUGAR 2"
        android:textSize="18sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Lugar1" />

    <Button
        android:id="@+id/Lugar1"
        android:layout_width="200dp"
        android:layout_height="77dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/verde"
        android:elevation="0dp"
        android:onClick="interactuar"
        android:text="LUGAR 1"
        android:textSize="18sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="77dp"
        android:layout_height="67dp"
        android:layout_below="@id/button4"
        android:visibility="visible"
        app:srcCompat="@drawable/est_logo"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintHorizontal_bias="0.786"
        app:layout_constraintTop_toBottomOf="@+id/Lugar4" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="140dp"
        android:layout_height="77dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/Lugar4"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:srcCompat="@drawable/barrera" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="79dp"
        android:layout_height="77dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/Lugar4"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:srcCompat="@drawable/flechas" />

    <TextView
        android:id="@+id/editText"
        android:layout_width="337dp"
        android:layout_height="41dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:lineSpacingExtra="8sp"
        android:text="HAY LUGAR"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="449dp" />

</android.support.constraint.ConstraintLayout>
